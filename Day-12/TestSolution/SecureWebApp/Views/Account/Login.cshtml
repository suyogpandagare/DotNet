
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>
 
    @using (Html.BeginForm())
    {
        <table>
            <tr>
                <td>Email:</td>
                <td><input type="email" name="email" /></td>
            </tr>
            <tr>
                <td>Password:</td>
                <td><input type="password" name="password" /></td>
            </tr>
            <tr>
                <td> </td>
                <td><button type="submit">Login</button></td>
            </tr>
        </table>
    }

 